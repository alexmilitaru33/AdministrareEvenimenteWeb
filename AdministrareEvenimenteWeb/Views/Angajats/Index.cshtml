@model IEnumerable<AdministrareEvenimenteWeb.Models.Angajat>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @*@Html.DisplayNameFor(model => model.Nume)*@
                @Html.ActionLink("Nume", "Index", new { sortOrder = ViewBag.NumeSortParm })
            </th>
            <th>
                @Html.ActionLink("Prenume", "Index", new { sortOrder = ViewBag.PrenumeSortParm })
               @* @Html.DisplayNameFor(model => model.Prenume)*@
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdSediuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdServiciiNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prenume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSediuNavigation.IdSediu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdServiciiNavigation.IdServicii)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAngajat">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAngajat">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAngajat">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
