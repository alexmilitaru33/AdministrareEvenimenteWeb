@model IEnumerable<AdministrareEvenimenteWeb.Models.LocatieEveniment>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumeLocatie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Judet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Localitate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Strada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numar)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumeLocatie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Judet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Localitate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Strada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numar)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdLocatieEveniment">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdLocatieEveniment">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdLocatieEveniment">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
